extends layout

block content
	.container
		h1= title
		p Welcome to #{title}
		.row
			.col-md-12.text-center.bg-danger
				script(src='http://player.twitch.tv/js/embed/v1.js').
				div(id='{PLAYER_DIV_ID}')
					script.
						var options = {
						width: 854,
						height: 480,
						channel: "reynad27"
						};
						var player = new Twitch.Player("{PLAYER_DIV_ID}", options);
						player.setVolume(0.5);


